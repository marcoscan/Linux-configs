URxvt.scrollstyle: plain
URxvt*scrollBar_right: true
URxvt*saveLines: 24000
urxvt.depth: 32

urxvt*shading: 15

!alternative config
!URxvt.font:xft:DejaVu Sans Mono:style=Regular:size=8,xft:DejaVu Sans Mono for Powerline
!URxvt.boldFont:xft:DejaVu Sans Mono:style=Regular:size=8

!actual config
URxvt*font: xft:DejaVu Sans Mono:pixelsize=11:antialias=true:hinting=true,xft:DejaVu Sans Mono for Powerline:pixelsize=11:antialias=true:hinting=true
!URxvt*boldFont: xft:DejaVu Sans Mono:style=bold:antialias=true:pixelsize=11
URxvt*italicFont: xft:DejaVu Sans Mono:style=italic:pixelsize=11:antialias=true:hinting=true,xft:DejaVu Sans Mono for Powerline:style=italic:pixelsize=11:antialias=true:hinting=true
!URxvt*boldItalicFont: xft:DejaVu Sans Mono:style=bolditalic:antialias=true:pixelsize=11

URxvt*buffered: false

URxvt.perl-ext-common:  default,clipboard,vtwheel,tabbed,url-select,keyboard-select

URxvt.tabbed.tabbar-fg: 3
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    2
URxvt.tabbed.tab-bg:    0

URxvt.keysym.C-Escape: perl:keyboard-select:activate
URxvt.keysym.C-s: perl:keyboard-select:search
URxvt.keysym.C-c:   perl:clipboard:copy
URxvt.keysym.C-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped

URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

!! *foreground: rgb:ff/ff/ff
!! *background: rgb:30/30/30
URxvt*foreground: rgb:ff/ff/ff
URxvt*background: rgb:30/30/30
!! blacks: Black / Dark grey
URxvt*color0:                #303030
URxvt*color8:                #3D3D3D
!! reds: Strong red / Light brilliant red
URxvt*color1:                #B80000
URxvt*color9:                #FF3333
!! greens: Light brilliant green / Light brilliant chartreuse green
URxvt*color2:                #afd700
URxvt*color10:               #afd700
!! yellows: Light brilliant orange / Light brilliant yellow
URxvt*color3:                #d0d0d0
URxvt*color11:               #FFFF33
!! blues: Light brilliant blue / Light brilliant azure
URxvt*color4:                #3333FF
URxvt*color12:               #3399FF
!! magentas: Light brilliant magenta / Light brilliant violet
URxvt*color5:                #FF33FF
URxvt*color13:               #9933FF
!! cyans: Light brilliant cyan / Very light cyan
URxvt*color6:                #33FFFF
URxvt*color14:               #99FFFF
!! whites: Cosmic latte / White
URxvt*color7:                #FFF8E7
URxvt*color15:               #FFFFFF
